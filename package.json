{
  "name": "timekeep",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint",
    "format": "eslint --fix",
    "stylelint": "stylelint src/**/**/*.css src/**/*.css",
    "check-duplicate-code": "jsinspect -t 20 src",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "concurrently 'npm:build:watch:ts' 'npm:build:watch:vite'",
    "build:watch:ts": "tsc --watch",
    "build:watch:vite": "vite build --watch",
    "serve": "vite preview",
    "test": "mocha-vite-puppeteer"
  },
  "dependencies": {
    "pouchdb": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "vue": "^3.1.1",
    "vue-class-component": "^8.0.0-rc.1",
    "vue-router": "^4.0.10",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.3.0-rc.11",
    "@storybook/addon-essentials": "^6.3.0-rc.11",
    "@storybook/addon-links": "^6.3.0-rc.11",
    "@storybook/vue3": "^6.3.0-rc.11",
    "@types/chai": "^4.2.19",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^6.3.6",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.1.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.29.0",
    "eslint-plugin-vue": "^7.11.1",
    "mocha": "^9.0.1",
    "mocha-vite-puppeteer": "^1.4.1",
    "postcss": "^8.3.5",
    "pouchdb-adapter-memory": "^7.2.2",
    "prettier": "^2.3.1",
    "storybook-builder-vite": "^0.0.9",
    "tailwindcss": "^2.2.2",
    "typescript": "^4.3.4",
    "vite": "^2.3.8",
    "vue-loader": "^16.2.0"
  }
}
