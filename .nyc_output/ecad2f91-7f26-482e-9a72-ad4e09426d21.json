T> {\n  private events: {[key: string]: EventEmitterCallback<T>[]};\n\n  constructor() {\n    this.events = {};\n  }\n\n  public on(event: string, listener: EventEmitterCallback<T>) {\n    if (!this.events[event])\n      this.events[event] = [];\n    this.events[event].push(listener);\n  }\n\n  public removeListener(event: string, listener: EventEmitterCallback<T>) {\n    if (this.events[event]) {\n      const index = this.events[event].indexOf(listener)\n      if (index > -1)\n        this.events[event].splice(index, 1);\n    }\n  }\n\n  public emit(event: string, data: T) {\n    if (this.events[event]) {\n      for (const callback of this.events[event])\n        callback.apply(this, [data]);\n    }\n  }\n}\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"962751fcb542ab5a346470cfef24c04797136eb8","contentHash":"89ba9b8edd40df9e53fcb90714310ffb0b5629cb70c7b686c397cfb7b459ed9c"},"/Users/alexgustafsson/Documents/GitHub/timekeep/src/state/project.ts":{"path":"/Users/alexgustafsson/Documents/GitHub/timekeep/src/state/project.ts","statementMap":{"0":{"start":{"line":2,"column":0},"end":{"line":2,"column":62}},"1":{"start":{"line":3,"column":16},"end":{"line":3,"column":32}},"2":{"start":{"line":4,"column":24},"end":{"line":4,"column":75}},"3":{"start":{"line":7,"column":8},"end":{"line":7,"column":16}},"4":{"start":{"line":8,"column":8},"end":{"line":8,"column":21}},"5":{"start":{"line":9,"column":8},"end":{"line":9,"column":25}},"6":{"start":{"line":12,"column":8},"end":{"line":12,"column":30}},"7":{"start":{"line":15,"column":25},"end":{"line":15,"column":39}},"8":{"start":{"line":16,"column":8},"end":{"line":16,"column":31}},"9":{"start":{"line":17,"column":8},"end":{"line":17,"column":92}},"10":{"start":{"line":20,"column":8},"end":{"line":20,"column":31}},"11":{"start":{"line":23,"column":25},"end":{"line":23,"column":40}},"12":{"start":{"line":24,"column":8},"end":{"line":24,"column":32}},"13":{"start":{"line":25,"column":8},"end":{"line":2